#include "stm32f1xx_hal.h"

#ifndef INC_EEPROM_STORAGE_H_
#define INC_EEPROM_STORAGE_H_

class EepromStorage {
private:
public:
	static HAL_StatusTypeDef read(I2C_HandleTypeDef* i2cx, uint16_t offset, size_t len, uint8_t* data);
	static HAL_StatusTypeDef write(I2C_HandleTypeDef* i2cx, uint16_t offset, size_t len, uint8_t* data);
};

#endif /* INC_EEPROM_STORAGE_H_ */
