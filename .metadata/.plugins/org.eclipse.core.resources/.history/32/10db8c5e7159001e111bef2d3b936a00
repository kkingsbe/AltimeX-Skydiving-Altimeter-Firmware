#ifndef INC_USB_H_
#define INC_USB_H_
#include "stm32f1xx_hal.h"

class USB {
private:
	UART_HandleTypeDef* uart
public:
	USB(UART_HandleTypeDef* uart);
	static void print(char* data, size_t len, UART_HandleTypeDef* uart);
	static void printf(float data, UART_HandleTypeDef* uart);
	static void printd(double data, UART_HandleTypeDef* uart);
	static void println(char* data, size_t len, UART_HandleTypeDef* uart);
};

#endif /* INC_USB_H_ */
