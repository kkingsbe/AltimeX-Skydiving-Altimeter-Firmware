#include "altimex_storage.h"

void read(uint16_t offset, size_t len, uint8_t* data)
{
	HAL_StatusTypeDef read_res = HAL_I2C_Mem_Read(&hi2c1, 0xA0, offset, 2, data, len, 1000);
}

