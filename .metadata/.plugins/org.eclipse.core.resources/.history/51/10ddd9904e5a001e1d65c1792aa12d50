#ifndef INC_ALTIMEX_EEPROM_H_
#define INC_ALTIMEX_EEPROM_H_

class AltimexEeprom {
private:
	enum offsets {
		CONFIGURATION = 0x0,
		LOGS = sizeof(AltimexConfig)
	};
public:
	void save_configuration();
};

#endif /* INC_ALTIMEX_EEPROM_H_ */
